    "stylelint-config-standard": "39.0.1",
    "stylelint-order": "7.0.0",
    "stylelint-selector-bem-pattern": "4.0.1",
    "tailwindcss": "4.1.17",
    "tw-animate-css": "1.4.0",
    "typed-query-selector": "2.12.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.14"
