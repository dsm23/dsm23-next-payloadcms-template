{
  "name": "dsm23-next-payloadcms-template",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "node .husky/install.js",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "next dev",
    "dev:prod": "rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "payload generate:importmap",
    "generate:types": "payload generate:types",
    "ii": "pnpm --ignore-workspace install",
    "fmt": "prettier . --write --cache --cache-strategy=content --cache-location=.prettiercache",
    "fmt.check": "prettier . --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "lint:css": "stylelint \"**/*.css\" --max-warnings 0 --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache",
    "lint:js": "next lint --max-warnings 0",
    "lint": "run-p lint:js lint:css",
    "lint:fix": "next lint --fix",
    "payload": "payload",
    "reinstall": "rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "next start",
    "test": "jest"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.28.1",
    "@payloadcms/db-mongodb": "3.28.1",
    "@payloadcms/live-preview-react": "3.28.1",
    "@payloadcms/next": "3.28.1",
    "@payloadcms/payload-cloud": "3.28.1",
    "@payloadcms/plugin-cloud-storage": "3.28.1",
    "@payloadcms/plugin-form-builder": "3.28.1",
    "@payloadcms/plugin-nested-docs": "3.28.1",
    "@payloadcms/plugin-redirects": "3.28.1",
    "@payloadcms/plugin-search": "3.28.1",
    "@payloadcms/plugin-seo": "3.28.1",
    "@payloadcms/richtext-lexical": "3.28.1",
    "@payloadcms/ui": "3.28.1",
    "@radix-ui/react-checkbox": "1.1.4",
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-select": "2.1.6",
    "@radix-ui/react-slot": "1.1.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "geist": "1.3.1",
    "graphql": "16.10.0",
    "lucide-react": "0.482.0",
    "minio": "8.0.5",
    "next": "15.2.2",
    "next-sitemap": "4.2.3",
    "payload": "3.28.1",
    "prism-react-renderer": "2.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "sharp": "0.33.5",
    "tailwind-merge": "3.0.2",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@commitlint/types": "19.8.0",
    "@eslint/eslintrc": "3.3.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@tailwindcss/postcss": "4.0.14",
    "@tailwindcss/typography": "0.5.16",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/escape-html": "1.0.4",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "commitizen": "4.3.1",
    "copyfiles": "2.4.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.22.0",
    "eslint-config-next": "15.2.2",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "npm-run-all2": "7.0.2",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "stylelint": "16.16.0",
    "stylelint-config-standard": "37.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-selector-bem-pattern": "4.0.1",
    "tailwindcss": "4.0.14",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.6.4+sha512.da3d715bfd22a9a105e6e8088cfc7826699332ded60c423b14ec613a185f1602206702ff0fe4c438cb15c979081ce4cb02568e364b15174503a63c7a8e2a5f6c",
  "engines": {
    "node": "18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
